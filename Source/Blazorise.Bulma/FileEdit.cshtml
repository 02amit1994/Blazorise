@inherits Base.BaseFileEdit
<Control Role="ControlRole.File">
    <FormLabel IsFile="true" For="@elementId">
        <input id="@elementId" type="file" class="@ClassMapper.Class" disabled="@IsDisabled" readonly="@IsReadonly" multiple="@IsMultiple" accept="@Filter" onchange="@PathChangedHandler" />
        @ChildContent
    </FormLabel>
</Control>

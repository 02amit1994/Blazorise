@inherits Base.BaseCheckEdit
<Control Role="@Role" IsInline="@IsInline">
    <FormLabel IsCheck="true" For="@elementId">
        <input type="@Type" class="@ClassMapper.Class" disabled="@IsDisabled" readonly="@IsReadonly" checked="@Checked" onchange="@CheckedChangedHandled" name="@RadioGroup" id="@elementId" />
        @ChildContent
    </FormLabel>
</Control>

@inherits Blazorise.BaseFileEdit
@if ( !HasCustomRegistration )
{
    <input id="@ElementId" type="file" class="@ClassMapper.Class" style="@StyleMapper.Style" disabled="@IsDisabled" readonly="@IsReadonly" multiple="@IsMultiple" accept="@Filter" @onchange="@PathChangedHandler" />
    @ChildContent
}
else
{
    @RenderCustomComponent()
}
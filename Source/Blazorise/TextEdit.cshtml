@* This has to be name TextEdit because Razor(not Blazor) still doesn't support case sensitive components. *@
@inherits Base.BaseTextEdit
@if ( !ComponentMapper.HasRegistration( this ) )
{
    <input ref="ElementRef" type="@Type" class="@ClassMapper.Class" placeholder="@Placeholder" disabled="@IsDisabled" readonly="@IsReadonly" maxlength="@MaxLength" value="@Text" oninput="@HandleOnInput" onchange="@HandleOnChange" />
    @ChildContent
}
else
{
    @RenderCustomComponent()
}
@* This has to be name SelectEdit because Razor(not Blazor) still doesn't support case sensitive components. *@
@inherits Base.BaseSelect
@if ( !ComponentMapper.HasRegistration( this ) )
{
    <select class="@ClassMapper.Class" disabled="@IsDisabled" readonly="@IsReadonly" multiple="@IsMultiple" onchange="@SelectionChangedHandler">
        @ChildContent
    </select>
}
else
{
    @RenderCustomComponent()
}


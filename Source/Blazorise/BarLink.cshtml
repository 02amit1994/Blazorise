@inherits Base.BaseBarLink
@if ( !ComponentMapper.HasRegistration( this ) )
{
    @if ( To != null )
    {
        <LinkBase class="@ClassMapper.Class" href="@To" title="@Title" Match="@Match">
            @ChildContent
        </LinkBase>
    }
    else
    {
        <a class="@ClassMapper.Class" title="@Title" onclick="@ClickHandler">
            @ChildContent
        </a>
    }
}
else
{
    @RenderCustomComponent()
}
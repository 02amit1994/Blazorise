@inherits Base.BaseBarToggler
<a role="button" class="@ClassMapper.Class" aria-label="Toggle navigation" onclick="@ClickHandler">
    @for ( int i = 0; i < TagProvider.BarTogglerSpanCount; ++i )
    {
        <span class="@ClassProvider.BarTogglerIcon()" aria-hidden="true"></span>
    }
    @ChildContent
</a>

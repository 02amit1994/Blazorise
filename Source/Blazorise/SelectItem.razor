@typeparam TValue
@inherits Blazorise.BaseSelectItem<TValue>
@if ( !HasCustomRegistration )
{
    <option class="@ClassNames" style="@StyleNames" value="@StringValue" selected="@IsSelected" disabled="@IsDisabled" @attributes="@Attributes">@ChildContent</option>
}
else
{
    @RenderCustomComponent()
}
@inherits BaseCheckEdit
@if ( !HasCustomRegistration )
{
    <input @ref="@ElementRef" id="@ElementId" type="@Type" class="@Classnames" style="@StyleMapper.Style" disabled="@IsDisabled" readonly="@IsReadonly" checked="@Checked" @onchange="@HandleCheckedChanged" name="@RadioGroup" />
    @ChildContent
    @Feedback
}
else
{
    @RenderCustomComponent()
}